<div class="container-fluid">
	<div class="row">
		<div class="twitchplayer">
      <h2><img class="logo" src="<%= @logo %>"></h2>
<iframe id="<%= @id %>" type="text/html" width="400px" height="300px"
  src="http://www.twitch.tv/<%= @channel %>/embed"
  frameborder="0"></iframe>
  <h4 class="user">User: <%= @channel %>   <strong class="views">viewers: <%= @views %></strong></h4>
  		</div>
  			<div class="profile">
          <h6 class="userpro"><%= current_user.name %></h6>
          <div class="profilepic"><img class="profilepic" src="<%= current_user.avatar.url.to_s %>"></div>
  				<%= link_to "Sign out", destroy_user_session_path, :method => :delete %>
  			</div>

	</div>
</div>